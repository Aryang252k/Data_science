    "num=input(\"Enter a number: \")\n",
    "nlen=len(num)\n",
    "temp=0\n",
    "pw=int(num[nlen-1])\n",
    "for i in range(nlen):\n",
    "  n=int(num[i])  \n",
    "  temp=temp+m.pow(n,nlen)\n",
    " \n",
    "if temp==int(num):\n",
    "  print(\"{} is armstrong number.\".format(num))\n",
    "else:\n",
    "  print(\"{} is not armstrong number.\".format(num))"
