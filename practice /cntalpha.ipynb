   "source": [
    "string=input(\"Enter the sting: \")\n",
    "st=\"\"\n",
    "s=string.upper()\n",
    "cnt=1\n",
    "for i in range(len(s)-1):\n",
    "  if s[i]==s[i+1] :\n",
    "   cnt=cnt+1\n",
    "  else:\n",
    "   st=st+str(cnt)+s[i]\n",
    "   cnt=1\n",
    "  if i+2==len(s):\n",
    "   st=st+str(cnt)+s[i+1] \n",
    "\n",
    "print(st)"
   ]
